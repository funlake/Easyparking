    </style>

    <style name="FormLine">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginTop">6dp</item>
        <item name="android:background">#FFF</item>
        <item name="android:minHeight">45dp</item>
    </style>

    <style name="DescLine">
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:minHeight">25dp</item>
    </style>

    <style name="DescText">
        <item name="android:textColor">@color/Highlight</item>
        <item name="android:textSize">14sp</item>
        <item name="android:shadowColor">#FFF</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">1</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Input">
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="AnimSlideBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/slide_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/slide_bottom_out</item>
    </style>
    <style name="AnimSlideRightLeft" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/slide_right_in</item>
        <item name="android:windowExitAnimation">@anim/slide_left_out</item>
    </style>

    <style name="AnimSlideRightRight" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/slide_right_in</item>
        <item name="android:windowExitAnimation">@anim/slide_right_out</item>
    </style>

    <style name="AnimAlphaInOut" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/alpha_in</item>
        <item name="android:windowExitAnimation">@anim/alpha_out</item>
    </style>

    <style name="SlideRightLeft" parent="Theme.AppCompat.Light">
        <item name="android:activatedBackgroundIndicator">@drawable/list_activated_background</item>
        <item name="android:windowAnimationStyle">@style/AnimSlideRightLeft</item>
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->

        <item name="android:actionModeCloseDrawable">@android:drawable/ic_menu_revert</item>
        <item name="android:subtitleTextStyle">@style/BarTitle</item>
        <item name="android:actionModeBackground">@color/Highlight</item>
    </style>

    <style name="SlideRightRight" parent="Theme.AppCompat.Light">
        <item name="android:activatedBackgroundIndicator">@drawable/list_activated_background</item>
        <item name="android:windowAnimationStyle">@style/AnimSlideRightRight</item>
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
        <item name="android:actionModeCloseDrawable">@android:drawable/ic_menu_revert</item>
        <item name="android:subtitleTextStyle">@style/BarTitle</item>
        <item name="android:actionModeBackground">@color/Highlight</item>
    </style>

</resources>