<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="TextTheme" parent="Theme.AppCompat.Light">

        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="android:textColor">#FF00FF</item>
    </style>
    <!-- Application theme. -->
    <style name="Base" parent="Theme.AppCompat.Light">
        <item name="android:activatedBackgroundIndicator">@drawable/list_activated_background</item>
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:actionModeCloseDrawable">@android:drawable/ic_menu_revert</item>
        <item name="android:subtitleTextStyle">@style/BarTitle</item>
        <item name="android:actionModeBackground">@color/Highlight</item>
    </style>

    <style name="Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowAnimationStyle">@style/SlideRightLeft</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/white</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="RatingBarSmall" parent="@android:style/Widget.RatingBar">         <item name="android:progressDrawable">@drawable/rating_bar</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:maxHeight">48dip</item>
    </style>

    <style name="Text">
        <item name="android:typeface">sans</item>
    </style>

    <style name="NormalTitle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">14sp</item>
        <item name="android:shadowColor">#8B8E86</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="BigTitle">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">20sp</item>
        <item name="android:shadowColor">#8B8E86</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="BarTitle">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">18sp</item>
        <item name="android:shadowColor">#ffffff</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="MenuTitle">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">15sp</item>
        <item name="android:shadowColor">#8B8E86</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">0</item>
        <item name="android:shadowRadius">0</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="ButtonTitle">
